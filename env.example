# OpenAI API Key (required)
OPENAI_API_KEY=sk-your-key-here

# Qdrant Cloud credentials (required)
QDRANT_URL=https://your-cluster-id.region.cloud.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key

# Qdrant collection name (optional, default: summa)
QDRANT_COLLECTION=summa

# Frontend URL for CORS (set in production)
FRONTEND_URL=https://your-app.vercel.app
